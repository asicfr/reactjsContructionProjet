{
  "esversion": 6,
  "extends": ["eslint-config-airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "rules": {
    "no-irregular-whitespace": 0,
    "linebreak-style": 0,
    "key-spacing": 0,
    "no-trailing-spaces": 0,
    "spaced-comment": 0,
    "no-tabs": 0,
    "no-mixed-spaces-and-tabs": 0,
    "jsx-indent-props": 0,
    "react/jsx-indent": 0,
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "no-class-assign": 0,
    "space-in-parens": 0,
    "global-require": 0,
    "react/prop-types": 0,
    "indent": 0,
    "padded-blocks": 0,
    "constructor-super": 2,
    "comma-dangle": 0,
    "eqeqeq": [2, "allow-null"],
    "id-length": 0,
    "no-eq-null": 0,
    "one-var": [2, { "initialized": "never" }],
    "prefer-const": 0,
    "no-param-reassign": 0,
    "no-this-before-super": 2,
    "babel/object-shorthand": 2,
    "react/jsx-boolean-value": 2,
    "react/jsx-no-duplicate-props": 2,
    "react/sort-comp": 0
  }
}